---
import Navbar from '~/components/TheNavbar.astro'
import RootLayout from '~/layouts/RootLayout.astro'
import Intro from './components/Intro.astro'
import Projects from './components/Projects.astro'
---

<RootLayout
  class="mx-auto max-w-[70rem] p-2 pb-10 sm:px-10"
  title="Jamal Shr Personal Portfolio website"
  description="I'm Jamal is a full-stack developer, designer, and content creator. He is passionate about building products and sharing his knowledge with the community."
>
  <main class="flex flex-col gap-8">
    <Navbar />
    <Intro />
    <Projects />
    <div class="h-20"></div>
  </main>
</RootLayout>
